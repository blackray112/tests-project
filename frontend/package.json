{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --open --live-reload --hot --config webpack/webpack.dev",
    "start:local": "cross-env NODE_ENV=development WATCHPACK_POLLING=true webpack serve --open --live-reload --hot --config webpack/webpack.local.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.dev",
    "lint": "eslint src --ext .ts,.tsx",
    "check-types:watch": "tsc --watch",
    "check-types": "tsc",
    "pre-commit": "yarn test",
    "test": "yarn check-types && yarn lint"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.32",
    "@types/react": "^18.2.7",
    "@types/redux-form": "^8.3.5",
    "axios": "^1.4.0",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.1",
    "external-remotes-plugin": "^1.0.0",
    "favicons-webpack-plugin": "^6.0.0",
    "file-loader": "^6.2.0",
    "final-form": "^4.20.9",
    "formik": "^2.2.9",
    "html-webpack-plugin": "^5.5.0",
    "webpack-merge": "^5.8.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-redux": "^8.0.5",
    "react-router-dom": "~6.3.0",
    "redux": "^4.2.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "web-vitals": "^2.1.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "yup": "0.30.0"
  },
  "devDependencies": {
    "@types/react-router": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "dotenv": "^16.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.8.8",
    "webpack-dev-server": "^4.7.4",
    "webpack-retry-chunk-load-plugin": "^3.1.1"
  },
  "resolutions": {
    "@types/react": "17.0.44",
    "@mui/lab": "5.0.0-alpha.77"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint",
      "yarn jest"
    ]
  }
}
